!! ~/.Xresources

!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------
Xft.dpi: 96
Xft.antialias: 1
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.lcdfilter: lcddefault

!!
!! rxvt-unicode (urxvt)
!!
! settings
URxvt.font: xft:PlemolJP Console NF:size=14:antialias=false, xft:DejaVu Sans Mono:size=14:antialias=false
URxvt.depth: 32
URxvt.geometry: 80x24
URxvt.transparent: false
URxvt.fading: 0
URxvt.loginShell: true
URxvt.internalBorder: 3
URxvt.lineSpace: 0
URxvt.scrollBar_right: false
URxvt.visualBell: false
URxvt.saveLines: 30000
URxvt.scrollBar: false
URxvt.iso14755: false

! cursor
URxvt.cursorBlink: true
URxvt.pointerBlank: true
URxvt.cursorUnderline: false
! Plugin
URxvt.perl-ext-common: default,resize-font,clipboard,matcher
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.matcher.launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.C-Down: resize-font:smaller
URxvt.keysym.C-Up: resize-font:bigger
URxvt.keysym.C-Right: resize-font:default

