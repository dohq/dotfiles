[core]
	editor = vim
	quotepath = false
	symlinks = true
	autocrlf = false
	ignorecase = false
	safecrlf = true
[color]
	ui = auto
	diff = always
	status = always
	branch = always
	interactive = always
[rebase]
	autosquash = true
[push]
	default = simple
[ghq]
	root = ~/go/src
[pull]
	ff = only
[merge]
	ff = false
[credential]
	helper = cache
