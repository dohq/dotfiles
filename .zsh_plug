# enhanced
# cdコマンドを便利にする。便利
zplug "b4b4r07/enhancd", use:enhancd.sh

# syntaxhilight
# コマンドにシンタックスハイライトを適用する。便利？
zplug "zsh-users/zsh-syntax-highlighting", nice:1

# comp
# コマンドを色々補完する。超便利
zplug "zsh-users/zsh-completions"
zplug "felixr/docker-zsh-completion"
zplug "sdurrheimer/docker-compose-zsh-completion"
zplug "leonhartX/docker-machine-zsh-completion"

# color
# 色付ける。便利
#zplug "seebi/dircolors-solarized"
zplug "chrissicool/zsh-256color"

# tmuxを便利に
zplug "arks22/tmuximum", as:command, use:bin/tmuximum
zplug "junegunn/fzf-bin", as:command, from:gh-r, rename-to:fzf
zplug "junegunn/fzf", as:command, use:bin/fzf-tmux
